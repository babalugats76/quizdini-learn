{
  "name": "server",
  "description": "Quizdini learn app",
  "config": {
    "comment": "Updated quizdini-learn source code"
  },
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "scripts": {
    "start": "node server/bin/www",
    "server": "nodemon server/bin/www",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prettier": "prettier **/*.{js,ts,vue,json} --write",
    "deploy-to-prod": "npm run prettier && npm run build --prefix client && git add . && git commit -m \"%npm_config_comment%\" && git push origin main && git push heroku main",
    "heroku-prebuild": "echo Hook for Heroku prebuild...",
    "heroku-postbuild": "echo Hook for Heroku postbuild..."
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-sslify": "^1.2.0",
    "http-status-codes": "^2.1.4",
    "memjs": "^1.2.2",
    "mongoose": "^5.10.17",
    "morgan": "~1.9.1",
    "vue-template-compiler": "^2.6.12"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1"
  }
}
